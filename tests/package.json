    "test:integration": "mocha --timeout 100000 --require ts-node/register tests/integration/**/*.test.ts",
    "test:e2e": "mocha --timeout 100000 --require ts-node/register tests/e2e/**/*.test.ts",
    "coverage": "ts-node coverage-report.ts"
  },
  "devDependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@solana/web3.js": "^1.87.6",
    "@solana/spl-token": "^0.3.9",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.6",
    "chai": "^4.3.10",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}

